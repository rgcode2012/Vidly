@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>
        @Model.MembershipType.Name
    </li>
    @if (Model.Bithdate.HasValue)
    {
        <li>@Model.Bithdate.Value.ToShortDateString()</li>
    }
</ul>

